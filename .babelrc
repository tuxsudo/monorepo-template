{
  "presets": [
    "@babel/typescript",
    "@babel/react",
    [
      "@babel/env",
      {
        "modules": "commonjs",
        "targets": {
          "browsers": "last 2 Chrome versions"
        }
      }
    ]
  ],

  "plugins": [
    [
      "styled-components",
      { "ssr": true, "displayName": true, "preprocess": false }
    ]
  ]
}
